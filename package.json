  "description": "React hooks for running code inside web workers without needing to eject CRA apps.",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/BlueBlazin/web-worker-hooks",
  "license": "MIT",
  "keywords": [],
  "scripts": {
