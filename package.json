    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.0.2",
    "@types/react": "^17.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
