{
  "name": "web-worker-hooks",
  "version": "0.3.0",
  "description": "React hooks for running code inside web workers without needing to eject CRA apps.",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/BlueBlazin/web-worker-hooks",
  "license": "MIT",
  "keywords": ["react", "web-worker", "hook", "web-worker-hooks"],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "watch:build": "tsc -p tsconfig.json -w"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.0.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "LICENSE",
    "README.md"
  ],
  "author": "blueblazin"
}
